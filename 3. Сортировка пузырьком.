#include <stdio.h>
int main() {
    int a;
    short int b[8]={27, -1, 12, -6, 5, 1, -3, 8};
    for (int i=0; i!=8; i++) {
        for (int c=0; c!=7; c++) {
            if (b[c]>b[c+1]) 
            {
                a=b[c];
                b[c]=b[c+1];
                b[c+1]=a;
            };
        };
    }
    for (int i=0; i!=8; i++ ) {        
        printf("%d ", b[i]);
    }
    return 0;
}

